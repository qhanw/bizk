{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "declaration": true,
    "skipLibCheck": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@bizk/hooks": ["./packages/hooks/src/index.ts"],
      "@bizk/components": ["./packages/components/src/index.ts"]
    }
  },
  "include": [
    "**/src/**/*"
    // "typings.d.ts",Ã¥
    // ".eslintrc.js",
    // "jest.config.js",
    // "**/fixtures"
  ]
}
